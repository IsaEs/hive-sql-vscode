{
    "name": "hive-sql",
    "version": "0.0.3",
    "displayName": "Hive SQL",
    "description": "Adds support for Hive's variant of the SQL language",
    "publisher": "josephtbradley",
    "author": {
        "name": "Joseph T. Bradley",
        "email": "jtbradley@gmail.com"
    },
    "license": "MIT",
    "icon": "./extras/apache_hive.png",
    "engines": {
        "vscode": "^1.20.0"
    },
    "galleryBanner": {
        "color": "#FBED20",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JoBrad/hive-sql-vscode.git"
    },
    "homepage": "https://github.com/JoBrad/hive-sql-vscode/blob/master/README.md",
    "categories": [
        "Programming Languages", "Language Packs"
    ],
    "keywords": [
        "Hive SQL", "Hive", "Apache", "SQL", "Impala", "syntax"
    ],
    "preview": true,
    "contributes": {
        "languages": [{
            "id": "hive-sql",
            "aliases": ["HiveSQL", "hive-sql", "Hive SQL"],
            "extensions": [".hql", ".q", ".ql"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "hive-sql",
            "scopeName": "source.hive",
            "path": "./syntaxes/hive.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "hive-sql",
                "path": "./snippets/hive-sql.json"
            }
        ]
    }
}
{
    "name": "hive-sql",
    "version": "0.0.1",
    "displayName": "hive-sql",
    "description": "Adds support for Hive's variant of the SQL language",
    "publisher": "josephtbradley",
    "author": {
        "name": "Joseph T. Bradley",
        "email": "jtbradley@gmail.com"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "hive-sql",
            "aliases": ["HiveSQL", "hive-sql", "Hive SQL"],
            "extensions": [".hql", ".q", ".ql"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "hive-sql",
            "scopeName": "source.hive",
            "path": "./syntaxes/hive.tmLanguage.json"
        }]
    }
}